// Define some routes.
TandemApp.config(["$routeProvider", "$locationProvider", function($routeProvider, $locationProvider) {
  $routeProvider.when('/home', {
    templateUrl: '<%= asset_path("index.html") %>'
  }).
  when('/', {
    templateUrl: '<%= asset_path("index.html") %>'
  }).
  otherwise({
    templateUrl: '<%= asset_path("index.html") %>'
  });

  $locationProvider.html5Mode(true);

}]);































































